call main;
endmacro;

main:
	if(true){
		//tabtotal[]で何故か-1が返る。
		openfile "/h";
	}else{
		//こちらは問題ない。
		openfile "";
	}
	
	insert("tabgrouptotal="+str(tabgrouptotal)+"\n");
	##i = 0;
	while( ##i < tabgrouptotal ) {
		##grouptop = gettabhandle( true, ##i, 0 );
		insert("tabtotal["+str(##grouptop)+"]=" + str(tabtotal[##grouptop])+"\n");
		##i = ##i + 1;
	}
	return ;
