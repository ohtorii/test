if(tabmode==false){
    message "タブモードで実行してください";
    endmacro;
}
debuginfo 1;

#i = 0;
while( #i < tabgrouptotal ) {
    #grouptop = gettabhandle( true, #i, 0 );
    debuginfo "==== Tab group ====";
    #j=0;
    while( #j < tabtotal[#grouptop] ) {
        #handle = gettabhandle( true, #i, #j );
        debuginfo "Title=" + gettitle(3,#handle);
        #j = #j + 1;
    }
    #i = #i + 1;
}
