main:
	debuginfo 1;
	debuginfo "==== getoutlineitem ====";
	##n=outlineitemcount;
	##i=0;
	while(##i<##n){
		$$text=getoutlineitem(##i,0);
		$$line=getoutlineitem(##i,1);
		$$level=getoutlineitem(##i,2);
		debuginfo $$text+" "+$$line+":"+$$level;

		##i = ##i + 1;
	}
	return ;

